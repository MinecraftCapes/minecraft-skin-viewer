{
  "name": "minecraft-skin-viewer",
  "version": "0.0.13",
  "description": "A Minecraft Skin Viewer with some extra features",
  "type": "module",
  "author": "james090500",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minecraftcapes/minecraft-skin-viewer.git"
  },
